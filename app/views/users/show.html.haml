.container.user-page
  %header.user-nav
    .media
      .media-left
        = image_tag @user.set_default_avatar, alt: "64x64", src: @user.avatar, data: {src: "holder.js/64x64", holder: {rendered: "true"}}, class: "media-object"
      .media-body
        %h4#top-aligned-media.media-heading
          = @user.name
          %span.label.label-default.project-number
            = @user.count_prototypes
            %i Projects
        .proto-user
          %span.degree
            = @user.participation
        .row
          .col-md-6
            %h4 Occupation
            %p
              = @user.occupation
          .col-md-6
            %h4 Profile
            %p
              = @user.profile
#masonry-container.container.proto-list
  .row
    = render @prototypes
.text-center
  = paginate @prototypes
